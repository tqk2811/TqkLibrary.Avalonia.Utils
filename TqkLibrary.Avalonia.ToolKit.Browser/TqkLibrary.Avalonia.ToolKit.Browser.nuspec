<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<!--https://docs.microsoft.com/en-us/nuget/reference/nuspec-->
	<metadata>
		<id>$id$</id>
		<version>1.0.0-build$buildDay$$buildIndex$</version>
		<description>my private lib</description>
		<authors>tqk2811</authors>
		<projectUrl>https://github.com/tqk2811/TqkLibrary.Avalonia.ToolKit</projectUrl>
		<repository type="git" url="https://github.com/tqk2811/TqkLibrary.Avalonia.ToolKit.git"/>
		<license type="expression">MIT</license>
		<readme>docs\README.md</readme>
		<dependencies>
			<group targetFramework="net8.0-browser1.0">
				<dependency id="Avalonia.Browser" version="11.3.8" />
				<dependency id="TqkLibrary.Avalonia.ToolKit" version="1.0.0-build20251201154401" />
			</group>
		</dependencies>
		<contentFiles>
			<files include="wwwroot\**\*.js"
				   buildAction="Content"
				   copyToOutput="true"
				   flatten="false"
				   />
		</contentFiles>
	</metadata>
	<files>
		<file src="..\README.md" target="docs\" />
		<file src="TqkLibrary.Avalonia.ToolKit.Browser.targets" target="build\" />
		<file src="wwwroot\**\*.js" target="contentFiles\any\any\wwwroot" />
		<file src="bin\Release\**\$id$.dll" target="lib\" />
		<file src="bin\Release\**\$id$.pdb" target="lib\" />
		<file src="bin\Release\**\$id$.xml" target="lib\" />
		<file src="**\*.cs" target="src\" exclude="obj\**\*.cs;bin\**\*.cs"/>
	</files>
</package>